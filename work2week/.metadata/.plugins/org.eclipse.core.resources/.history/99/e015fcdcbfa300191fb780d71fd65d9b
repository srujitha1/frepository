<?xml version="1.0" encoding="UTF-8"?>
<configuration debug - "true">
 	<appender name = "STDOUT" class = "ch.qol.logback.core.ConsoleAppender">
 		<encodeer>
 			<pattern>%d{HH:mm:ss.SSS} [%thread] %-5level %logger{36} - %msg</pattern>
 		</encodeer>
 
 	</appender>
 	<logger name ="slf4jdemo" level = "debug" > </logger>
 	<root level="INFO">
 	<appender-ref ref="STDOUT" />
 	</root>	
 </configuration>